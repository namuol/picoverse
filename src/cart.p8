  -- light_gray,
  light_gray,
  -- dark_gray,
  dark_gray,

  dark_purple,
  dark_blue,
  blue,
  -- green,
  dark_green,
  -- red,
}

function random_face_props()
  local skintone = choose(skintone_probs)

  local hairtone = skintone
  while hairtone == skintone do
    hairtone = choose(hairtone_probs)
  end

  return {
    skintone=skintone,
    hairtone=hairtone,
    hair=choose(hair_probs),
    facial_hair=choose(facial_hair_probs),
